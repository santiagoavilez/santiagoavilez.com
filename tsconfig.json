{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    "types": [
        "astro/client"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "paths": {
    "@components/*": [
      "src/components/*"
    ],
    "public/*": [
      "public/*"
    ],
    "@assets/*": [
        "src/assets/*"
      ],
    "@lib/*": [
        "./src/lib/*"
    ],
    }
  },

}